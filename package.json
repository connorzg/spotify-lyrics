{
  "name": "spotify-lyrics",
  "version": "1.1.0",
  "description": "Lyrics for Spotify",
  "main": "main.js",
  "scripts": {
    "test": "mocha",
    "app": "electron app",
    "dev": "webpack -w",
    "build": "cross-env NODE_ENV=production webpack",
    "pack": "npm run build && build --dir && babel --no-comments --compact --minified --out-file app/dist/bundle.js app/dist/bundle.js",
    "dist": "npm run build && build"
  },
  "build": {
    "appId": "com.andredieb.spotify-lyrics",
    "asar": false,
    "extend-info": "build/Info.plist",
    "linux": {
      "arch": [
        "ia32",
        "x64"
      ],
      "target": [
        "deb",
        "AppImage",
        "rpm"
      ]
    },
    "win": {
      "target": [
        "squirrel"
      ]
    },
    "mac": {
      "category": "public.app-category.music"
    }
  },
  "repository": "dieb/spotify-lyrics",
  "keywords": [
    "spotify",
    "lyrics"
  ],
  "author": "Andre Dieb <andre.dieb@gmail.com> (http://andredieb.com)",
  "license": "CC0-1.0",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.3",
    "electron": "^1.4.12",
    "electron-builder": "^10.8.1",
    "mocha": "^3.2.0",
    "spectron": "^3.4.0",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "electron-is-dev": "0.1.2",
    "forever-monitor": "^1.7.1",
    "json-loader": "0.5.4",
    "ps-node": "^0.1.4",
    "underscore": "^1.8.3",
    "vue": "2.1.3",
    "xmldom": "^0.1.27",
    "xpath": "0.0.23"
  }
}
